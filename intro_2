print(type(10))
print(type(3.14))
print(type(1 + 3j))
print(type('vinod'))
print(type([1, 2, 3]))
print(type({'name':'vinod'}))
print(type({9.8, 3.14, 2.7}))
print(type((9.8, 3.14, 2.7)))
